(function($){'use strict';var
$offers=$('article.offer'),$term_list=$('.term-list');function combineTwoArrays(arr1,arr2){var arr_result=[];if(arr1.length==0)return arr2;if(arr2.length==0)return arr1;for(var index1 in arr1){for(var index2 in arr2){var e1=arr1[index1];var e2=arr2[index2];if(!$.isArray(e1))e1=[e1];if(!$.isArray(e2))e2=[e2];arr_result.push(e1.concat(e2));}}
return arr_result;}
function combineArrays(){var arr_result=[];for(var index in arguments){arr_result=combineTwoArrays(arr_result,arguments[index]);}
return arr_result;}

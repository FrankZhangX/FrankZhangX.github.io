var WheelIndicator=(function(win,doc){function extend(defaults,options){var extended={},prop;for(prop in defaults){if(Object.prototype.hasOwnProperty.call(defaults,prop)){extended[prop]=defaults[prop];}}
for(prop in options){if(Object.prototype.hasOwnProperty.call(options,prop)){extended[prop]=options[prop];}}
return extended;}
var eventWheel='onwheel'in doc?'wheel':'mousewheel',DEFAULTS={callback:function(){},elem:doc,preventMouse:true};function Module(options){this._options=extend(DEFAULTS,options);this._deltaArray=[0,0,0];this._isAcceleration=false;this._isStopped=true;this._direction='';this._timer='';this._isWorking=true;var self=this;addEvent(this._options.elem,eventWheel,function(event){if(self._isWorking){processDelta.call(self,event);if(self._options.preventMouse)preventDefault(event);}});}
Module.prototype={Constructor:Module,turnOn:function(){this._isWorking=true;return this;},turnOff:function(){this._isWorking=false;return this;},setOptions:function(options){this._options=extend(this._options,options);return this;},getOption:function(option){var neededOption=this._options[option];if(neededOption!==undefined)return neededOption;throw new Error('Unknown option');}};function triggerEvent(event){event.direction=this._direction;this._options.callback.call(this,event);}
var getDeltaY=function(event){if(event.wheelDelta){getDeltaY=function(event){return event.wheelDelta*-1;};}else{getDeltaY=function(event){return event.deltaY;};}
return getDeltaY(event);};function preventDefault(event){event=event||win.event;if(event.preventDefault){event.preventDefault();}else{event.returnValue=false;}}
function processDelta(event){var
self=this,delta=getDeltaY(event),direction=delta>0?'down':'up',arrayLength=self._deltaArray.length,changedDirection=false,repeatDirection=0,sustainableDirection,i;clearTimeout(self._timer);self._timer=setTimeout(function(){self._deltaArray=[0,0,0];self._isStopped=true;self._direction=direction;},150);for(i=0;i<arrayLength;i++){if(self._deltaArray[i]!==0){self._deltaArray[i]>0?++repeatDirection:--repeatDirection;}}
if(Math.abs(repeatDirection)===arrayLength){sustainableDirection=repeatDirection>0?'down':'up';if(sustainableDirection!==self._direction){changedDirection=true;self._direction=direction;}}
if(!self._isStopped){if(changedDirection){self._isAcceleration=true;triggerEvent.call(this,event);}else{if(Math.abs(repeatDirection)===arrayLength){analyzeArray.call(this,event);}}}
if(self._isStopped){self._isStopped=false;self._isAcceleration=true;self._direction=direction;triggerEvent.call(this,event);}
self._deltaArray.shift();self._deltaArray.push(delta);}
function analyzeArray(event){var
deltaArray0Abs=Math.abs(this._deltaArray[0]),deltaArray1Abs=Math.abs(this._deltaArray[1]),deltaArray2Abs=Math.abs(this._deltaArray[2]),deltaAbs=Math.abs(getDeltaY(event));if((deltaAbs>deltaArray2Abs)&&(deltaArray2Abs>deltaArray1Abs)&&(deltaArray1Abs>deltaArray0Abs)){if(!this._isAcceleration){triggerEvent.call(this,event);this._isAcceleration=true;}}
if((deltaAbs<deltaArray2Abs)&&(deltaArray2Abs<=deltaArray1Abs)){this._isAcceleration=false;}}
function addEvent(elem,type,handler){if(win.addEventListener){elem.addEventListener(type,handler,false);}else if(win.attachEvent){elem.attachEvent('on'+type,handler);}}
return Module;}(window,document));if(typeof exports==='object'){module.exports=WheelIndicator;};
(function($){'use strict';var
scroll_animation_running=false,scrolled_intro_away=false,request_animation_frame=window.requestAnimationFrame||window.webkitRequestAnimationFrame||window.mozRequestAnimationFrame||window.msRequestAnimationFrame||window.oRequestAnimationFrame||function(callback){window.setTimeout(callback,1000/60)},last_scroll_top=-1,$window=$(window),$full_screen_intro=$('.full-screen-intro').first(),$document_header=$full_screen_intro.find('.header-bar').first(),$scroll_hint=$full_screen_intro.find('.scroll-to-body').first(),$carousel=$full_screen_intro.find('#full-screen-intro-carousel'),$intro_bottom=$full_screen_intro.find('.intro-bottom').first(),document_header_top=false,$hold_in_viewport=$('.hold-in-viewport'),sticky_class='sticky';if(!$full_screen_intro.length)return false;$window.on('load resize',function(){document_header_top=$document_header.offset().top;});function disableScrolling(){window.onwheel=window.onmousewheel=document.onmousewheel=window.ontouchmove=document.onkeydown=function(event){event.preventDefault();return false};}
function enableScrolling(){window.onmousewheel=document.onmousewheel=window.onwheel=window.ontouchmove=document.onkeydown=null;}
function scrollIntroAway(event){if(scrolled_intro_away||scroll_animation_running)return;unregisterScrollEvents();if(document_header_top){window.scrollTo(0,0);scroll_animation_running=true;disableScrolling();$('html, body').animate({'scrollTop':document_header_top+1},500).promise().done(function(){scrolled_intro_away=true;scroll_animation_running=false;enableScrolling();});}
else return;$scroll_hint.fadeOut();event.preventDefault();}
function resetScrollState(){unregisterScrollEvents();scroll_animation_running=true;scrolled_intro_away=false;disableScrolling();$('html, body').animate({'scrollTop':0},500).promise().done(function(){scroll_animation_running=false;last_scroll_top=-1;enableScrolling();$scroll_hint.show();registerScrollDownEvents();});}
function calculateTaglineOpacity(event){var
$caption=$full_screen_intro.find('.site-name-tagline:first').first(),$tagline=$full_screen_intro.find('.tagline:first').first(),intro_bottom_top=$intro_bottom.offset().top,intro_bottom_height=$tagline.outerHeight(),opacity=1-(window.pageYOffset-intro_bottom_top)/intro_bottom_height,opacity=Math.max(0,opacity),opacity=Math.min(opacity,1);if(!$tagline.length)return false;if(opacity<1)
$tagline.css('opacity',opacity);else
$tagline.css('opacity','');}
function keepElementsInViewport(event){$hold_in_viewport.each(function(index,object){var
$element=$(object),element_init_top=$element.data('init-top');if(window.pageYOffset>=element_init_top&&!$element.hasClass(sticky_class)){$element.addClass(sticky_class);}
else if(window.pageYOffset<element_init_top&&$element.hasClass(sticky_class)){$element.removeClass(sticky_class);}});}
function animateNextFrame(){if(last_scroll_top!=window.pageYOffset){last_scroll_top=window.pageYOffset;keepElementsInViewport();calculateTaglineOpacity();if($scroll_hint.offset().top<last_scroll_top){$scroll_hint.hide();}
if(scrolled_intro_away&&$intro_bottom.offset().top>last_scroll_top){resetScrollState();}}
request_animation_frame(animateNextFrame);}
function wrapImages(){$full_screen_intro.find('img.original-intro-image').each(function(index,image){var
$img=$(image),$wrapper=$('<div />').addClass('intro-image').css('background-image','url("'+$img.attr('src')+'")');$img.wrap($wrapper);});}
function setInitTopPositions(){$window.on('load resize',function(){$hold_in_viewport.each(function(index,object){$(object).data('init-top',$(object).offset().top);});});}
function registerScrollDownEvents(){$window.one('scroll',scrollIntroAway);$full_screen_intro.one('click',scrollIntroAway);new WheelIndicator({elem:$full_screen_intro.get(0),callback:function(event){if(event.direction=='down'){this.turnOff();scrollIntroAway(event);}}});new WheelIndicator({elem:window,callback:function(event){if(scrolled_intro_away){this.turnOff();}}});}
function unregisterScrollEvents(){$window.off('scroll',scrollIntroAway);$full_screen_intro.off('click',scrollIntroAway);}
wrapImages();setInitTopPositions();if($window.width()>=992&&window.pageYOffset<10){registerScrollDownEvents();}
$window.on('load',function(){$full_screen_intro.imagesLoaded(function(event){$full_screen_intro.find('img.logo, .site-name-tagline, .intro-image').animate({'opacity':1},1500).end().removeClass('nnl-loader');$scroll_hint.click(scrollIntroAway).delay(5000).fadeIn();});});$window.on('load',animateNextFrame);}(jQuery));
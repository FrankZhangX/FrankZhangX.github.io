(function($){'use strict';$.fn.ajaxForm=function(options){return this.each(function(){new AJAX_Form(this,options);});};function AJAX_Form(form_element,options){var self=this;self.defaults={};self.form_element=form_element;self.$form=$(form_element);self.form_states={ready:'state-ready',process:'state-processing',done:'state-done'};this.settings=$.extend({},self.defaults,options);this.init=function(){self.$form.on('submit',self.submitEvent);self.setFormState('ready');}
this.setFormState=function(new_state){for(var state_name in self.form_states){var state_class=self.form_states[state_name];self.$form.removeClass(state_class);}
self.$form.addClass(self.form_states[new_state]);}
this.submitEvent=function(event){event.preventDefault();self.hideMessages();self.disableForm();self.setFormState('process');self.submitForm();}
this.submitForm=function(){var
form_action=self.$form.attr('action'),form_data=self.$form.serializeArray();$.post(form_action,form_data,self.receiveResponse,'json');}
this.hideMessages=function(){self.$form.find('.success, .failure').hide().end().find('[data-issue-for]').slideUp('normal',function(event){$(this).html('')});}
this.showSuccessMessage=function(){self.$form.find('.success').slideDown();}
this.showFailureMessage=function(){self.$form.find('.failure').slideDown();}
this.showFieldIssue=function(field,issue){self.$form.find('[data-issue-for='+field+']').stop(true,true).html(issue).slideDown();}
this.disableForm=function(complete){if(complete)
var selector='button, input, select, textarea';else
var selector='[type=submit]';self.$form.find(selector).prop('disabled',true);}
this.enableForm=function(){self.$form.find('[type=submit]').prop('disabled',false);}
this.receiveResponse=function(data,textStatus,jqXHR){for(var field in data.issues){var issue=data.issues[field];self.showFieldIssue(field,issue);}
if(data.success){self.setFormState('done');self.disableForm(true);self.showSuccessMessage();var
client_redirect=self.$form.data('success-redirect'),server_redirect=data.redirect;if(typeof client_redirect!='undefined'&&client_redirect!='')
window.location.href=client_redirect;else if(typeof server_redirect!='undefined'&&server_redirect!='')
window.location.href=server_redirect;}
else{self.showFailureMessage();self.enableForm();self.setFormState('ready');}}
